                    },
                    "province": {
                      "type": "string",
                      "example": "Provincia"
                    },
                    "country": {
                      "type": "string",
                      "example": "País"
                    }
                  }
                },
                "geolocation": {
                  "type": "object",
                  "properties": {
                    "latitude": {
                      "type": "string",
                      "example": "-34.603722"
                    },
                    "longitude": {
                      "type": "string",
                      "example": "-58.381592"
                    }
                  }
                },
                "rooms": {
                  "type": "string",
                  "example": "5"
                },
                "bedrooms": {
                  "type": "string",
                  "example": "3"
                },
                "bathrooms": {
                  "type": "string",
                  "example": "2"
                },
                "hasTerrace": {
                  "type": "boolean",
                  "example": true
                },
                "hasBalcony": {
                  "type": "boolean",
                  "example": false
                },
                "garage": {
                  "type": "string",
                  "example": "1"
                },
                "hasStorageRoom": {
                  "type": "boolean",
                  "example": true
                },
                "age": {
                  "type": "string",
                  "example": "20"
                },
                "propertyType": {
                  "type": "string",
                  "enum": [
                    "casa",
                    "ph",
                    "departamento",
                    "local",
                    "oficina",
                    "galpon",
                    "terreno"
                  ]
                },
                "squareMeters": {
                  "type": "object",
                  "properties": {
                    "covered": {
                      "type": "string",
                      "example": "100"
                    },
                    "semiCovered": {
                      "type": "string",
                      "example": "50"
                    },
                    "uncovered": {
                      "type": "string",
                      "example": "30"
