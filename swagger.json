                "amenities": {
                  "type": "array",
                  "example": [
                    "quincho",
                    "pileta",
                    "jacuzzi",
                    "sauna",
                    "SUM",
                    "sala de juegos"
                  ],
                  "items": {
                    "type": "string"
                  }
                },
                "photos": {
                  "type": "array",
                  "example": [
                    "http://example.com/photo1.jpg",
                    "http://example.com/photo2.jpg"
                  ],
                  "items": {
                    "type": "string"
                  }
                },
                "video": {
                  "type": "string",
                  "example": "http://example.com/videotour.mp4"
