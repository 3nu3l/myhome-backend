                "price": {
                  "type": "string",
                  "example": "250000.00"
                },
                "expensesPrice": {
                  "type": "string",
                  "example": "5000.00"
                },
                "status": {
                  "type": "string",
                  "enum": [
                    "en alquiler",
                    "en venta",
                    "reservada",
                    "alquilada",
                    "vendida"
